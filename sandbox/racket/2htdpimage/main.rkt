#lang racket
(require 2htdp/image)
(define border (pen (color 255 192 192) 32 "solid" "round" "round"))
(define border1 (pen (color 255 255 255) 16 "solid" "round" "round"))
(define img (bitmap/file "tity1.png"))
(set! img (add-line img 0 0 400 0 border))
(set! img (add-line img 400 0 400 400 border))
(set! img (add-line img 400 400 0 400 border))
(set! img (add-line img 0 400 0 0 border))
(set! img (add-line img 0 0 400 0 border1))
(set! img (add-line img 400 0 400 400 border1))
(set! img (add-line img 400 400 0 400 border1))
(set! img (add-line img 0 400 0 0 border1))
(save-image img "a.png")